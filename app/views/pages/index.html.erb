<!-- app/views/pages/index.html.erb -->

<h1>Data Collection</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Email</th>
      <!-- Add more columns as needed -->
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <!-- Add more columns as needed -->
      </tr>
    <% end %>
  </tbody>
</table>
